<?xml version="1.0" encoding="utf-8"?>
<templates>
    <group t-name="wysiwyg.buttons.list">
        <button name="list-ul" title="Unordered list"
            data-method="insertList" data-value="ul">
            <i class="fa fa-list-ul"></i>
        </button>
        <button name="list-ol" title="Ordered list"
            data-method="insertList" data-value="ol">
            <i class="fa fa-list-ol"></i>
        </button>
        <button name="list-checklist" title="Checklist"
            data-method="insertList" data-value="checklist">
            <i class="fa fa-check-square"></i>
        </button>
    </group>
</templates>
