<?xml version="1.0"?>
<odoo>
    <data>
        <record id="im_livechat_channel_rule_demo" model="im_livechat.channel.rule">
            <field name="regex_url">/im_livechat/</field>
            <field name="action">auto_popup</field>
            <field name="auto_popup_timer">3</field>
            <field name="channel_id" ref="im_livechat_channel_data" />
        </record>

        <record id="im_livechat.im_livechat_group_user" model="res.groups">
            <field name="users" eval="[(4, ref('base.user_demo'))]"/>
        </record>

        <record id="im_livechat_channel_data" model="im_livechat.channel">
             <field name="user_ids" eval="[(4, ref('base.user_admin'))]"/>
        </record>

        <record id="mail_channel_demo_1" model="mail.channel">
            <field name="name">Visitor, Mitchell Admin</field>
            <field name="livechat_operator_id" ref="base.partner_admin"/>
            <field name="livechat_channel_id" ref="im_livechat.im_livechat_channel_data"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=1)"></field>
            <field name="channel_type">livechat</field>
        </record>

        <record id="mail_channel_demo_2" model="mail.channel">
            <field name="name">Visitor, Mitchell Admin</field>
            <field name="livechat_operator_id" ref="base.partner_admin"/>
            <field name="livechat_channel_id" ref="im_livechat.im_livechat_channel_data"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=3)"></field>
            <field name="channel_type">livechat</field>
        </record>

        <record id="mail_message_demo_1" model="mail.message">
            <field name="author_id" eval="False"/>
            <field name="record_name">Visitor</field>
            <field name="date" eval="datetime.now() - timedelta(days=1)"></field>
            <field name="create_date" eval="datetime.now() - timedelta(days=1)"></field>
            <field name="body"><![CDATA[<p>Hi</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_1"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_1])]"/>
        </record>

        <record id="mail_message_demo_2" model="mail.message">
            <field name="author_id" ref="base.partner_admin"/>
            <field name="date" eval="datetime.now() - timedelta(days=1, seconds=-15)"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=1, seconds=-15)"/>
            <field name="body"><![CDATA[<p>Hello</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_1"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_1])]"/>
        </record>

        <record id="mail_message_demo_3" model="mail.message">
            <field name="author_id" eval="False"/>
            <field name="record_name">Visitor</field>
            <field name="date" eval="datetime.now() - timedelta(days=1, seconds=-30)"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=1, seconds=-30)"/>
            <field name="body"><![CDATA[<p>Rating: üòä</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_1"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_1])]"/>
        </record>

        <record id="mail_message_demo_4" model="mail.message">
            <field name="author_id" eval="False"/>
            <field name="record_name">Visitor</field>
            <field name="date" eval="datetime.now() - timedelta(days=3)"></field>
            <field name="create_date" eval="datetime.now() - timedelta(days=3)"></field>
            <field name="body"><![CDATA[<p>Hello</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_2"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_2])]"/>
        </record>

        <record id="mail_message_demo_5" model="mail.message">
            <field name="author_id" ref="base.partner_admin"/>
            <field name="date" eval="datetime.now() - timedelta(days=3, seconds=-10)"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=3, seconds=-10)"/>
            <field name="body"><![CDATA[<p>Hey, how can I help you?</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_2"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_2])]"/>
        </record>

        <record id="mail_message_demo_6" model="mail.message">
            <field name="author_id" eval="False"/>
            <field name="record_name">Visitor</field>
            <field name="date" eval="datetime.now() - timedelta(days=3, seconds=-20)"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=3, seconds=-20)"/>
            <field name="body"><![CDATA[<p>Rating: üòê</p>]]></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_2"/>
            <field name="model">mail.channel</field>
            <field name="channel_ids" eval="[(6, False, [mail_channel_demo_2])]"/>
        </record>

        <record id="rating_rating_1" model="rating.rating">
            <field name="res_model_id" ref="mail.model_mail_channel"/>
            <field name="rated_partner_id" ref="base.partner_admin"/>
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=1)"></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_1"/>
            <field name="rating" eval="10"/>
            <field name="consumed" eval="True"/>
        </record>

        <record id="rating_rating_2" model="rating.rating">
            <field name="res_model_id" ref="mail.model_mail_channel"/>
            <field name="rated_partner_id" ref="base.partner_admin"/>
            <field name="partner_id" ref="base.partner_admin"/>
            <field name="create_date" eval="datetime.now() - timedelta(days=3)"></field>
            <field name="res_id" ref="im_livechat.mail_channel_demo_2"/>
            <field name="rating" eval="5"/>
            <field name="consumed" eval="True"/>
        </record>

    </data>
</odoo>
