        </record>

        <!-- Address Format:
                street
                street2
                City Zip
                State_name
                Country_name -->
        <record id="view_partner_address4_inherit" model="ir.ui.view">
            <field name="name">view partner address4 form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_address"/>
            <field name="mode">primary</field>
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('o_address_format')]" position="attributes">
                    <attribute name="class">o_address_format o_zip_city</attribute>
                </xpath>
                <xpath expr="//field[@name='zip']" position="replace"/>
                <xpath expr="//field[@name='city']" position="after">
                    <field name="zip" placeholder="ZIP" class="o_address_zip"/>
                </xpath>
                <xpath expr="//field[@name='state_id']" position="replace"/>
                <xpath expr="//field[@name='country_id']" position="before">
                    <div>
                        <field name="state_id" placeholder="state id"/>
                    </div>
                </xpath>
            </field>
        </record>
        <record id="base.in" model="res.country">
